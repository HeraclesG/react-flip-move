{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [true, "never-prefix"],
        "quotemark": [true, "single", "jsx-double"],
        "array-type": [true, "generic"],
        "no-empty": false,
        "max-line-length": [true, 140],
        "member-ordering": [false],
        "no-namespace": [false],
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}